\tab\tab\tab function validatePauseTimings()\par
\tab\tab\tab\{\tab\tab\tab\par
\tab\tab\tab\tab var form = document.getElementById("form");\par
\tab\tab\tab\par
\tab\tab\tab\tab var arrayExtraPauseHH = form.querySelectorAll("input[name='extraPauseHH[]']");\par
\tab\tab\tab\tab var arrayExtraPauseMM = form.querySelectorAll("input[name='extraPauseMM[]']");\par
\tab\tab\tab\tab var arrayExtraPauseSS = form.querySelectorAll("input[name='extraPauseSS[]']");\par
\tab\tab\tab\tab var arrayExtraPlayHH = form.querySelectorAll("input[name='extraPlayHH[]']");\par
\tab\tab\tab\tab var arrayExtraPlayMM = form.querySelectorAll("input[name='extraPlayMM[]']");\par
\tab\tab\tab\tab var arrayExtraPlaySS = form.querySelectorAll("input[name='extraPlaySS[]']");\par
\tab\tab\tab\tab\par
\tab\tab\tab\tab var syncStartTime = parseInt(form.syncStartTimeHH.value,10) * 3600 + parseInt(form.syncStartTimeMM.value,10) * 60 + parseInt(form.syncStartTimeSS.value,10);\par
\tab\tab\tab\tab var syncStopTime = parseInt(form.syncStopTimeHH.value,10) * 3600 + parseInt(form.syncStopTimeMM.value,10) * 60 + parseInt(form.syncStopTimeSS.value,10);\par
\tab\tab\tab\tab\par
\tab\tab\tab\tab\par
\tab\tab\tab\tab for(var i = 0; i < arrayExtraPauseHH.length; i++)\par
\tab\tab\tab\tab\{\par
\tab\tab\tab\tab\tab arrayExtraPauseHH[i].setCustomValidity("");\par
\tab\tab\tab\tab\tab arrayExtraPauseMM[i].setCustomValidity("");\par
\tab\tab\tab\tab\tab arrayExtraPauseSS[i].setCustomValidity("");\par
\tab\tab\tab\tab\tab arrayExtraPlayHH[i].setCustomValidity("");\par
\tab\tab\tab\tab\tab arrayExtraPlayMM[i].setCustomValidity("");\par
\tab\tab\tab\tab\tab arrayExtraPlaySS[i].setCustomValidity("");\par
\tab\tab\tab\tab\tab\par
\tab\tab\tab\tab\tab var pauseStartTime = parseInt(arrayExtraPauseHH[i].value,10) * 3600 + parseInt(arrayExtraPauseMM[i].value,10) * 60 + parseInt(arrayExtraPauseSS[i].value,10);\par
\tab\tab\tab\tab\tab var pauseResumeTime = parseInt(arrayExtraPlayHH[i].value,10) * 3600 + parseInt(arrayExtraPlayMM[i].value,10) * 60 + parseInt(arrayExtraPlaySS[i].value,10);\par
\tab\tab\tab\tab\tab\par
\tab\tab\tab\tab\tab // check that sync start < pause start (< sync stop)\par
\tab\tab\tab\tab\tab if(pauseStartTime <= syncStartTime)\par
\tab\tab\tab\tab\tab\{\par
\tab\tab\tab\tab\tab\tab arrayExtraPauseMM[i].setCustomValidity("Pause timer incorrect: pause starts before the start of the synchronized reaction");\par
\tab\tab\tab\tab\tab\}\par
\tab\tab\tab\tab\tab\par
\tab\tab\tab\tab\tab if(syncStopTime > 0 && pauseStartTime >= syncStopTime)\par
\tab\tab\tab\tab\tab\{\par
\tab\tab\tab\tab\tab\tab arrayExtraPauseMM[i].setCustomValidity("Pause timer incorrect: pause starts after the end of the synchronized reaction");\par
\tab\tab\tab\tab\tab\}\par
\tab\tab\tab\tab\tab\par
\tab\tab\tab\tab\tab // check that pause start > previous pause resume\par
\tab\tab\tab\tab\tab if(i > 0)\par
\tab\tab\tab\tab\tab\{\par
\tab\tab\tab\tab\tab\tab var previousPauseResumeTime = parseInt(arrayExtraPlayHH[i-1].value,10) * 3600 + parseInt(arrayExtraPlayMM[i-1].value,10) * 60 + parseInt(arrayExtraPlaySS[i-1].value,10);\par
\tab\tab\tab\tab\tab\tab\par
\tab\tab\tab\tab\tab\tab if(pauseStartTime < previousPauseResumeTime)\par
\tab\tab\tab\tab\tab\tab\{\par
\tab\tab\tab\tab\tab\tab\tab arrayExtraPauseMM[i].setCustomValidity("Pause timer incorrect: pause can't start before the previous one ended");\par
\tab\tab\tab\tab\tab\tab\}\par
\tab\tab\tab\tab\tab\}\par
\tab\tab\tab\tab\tab\par
\tab\tab\tab\tab\tab // check that pause resume time < sync stop\par
\tab\tab\tab\tab\tab if(syncStopTime > 0 && pauseResumeTime >= syncStopTime)\par
\tab\tab\tab\tab\tab\{\par
\tab\tab\tab\tab\tab\tab arrayExtraPlayMM[i].setCustomValidity("Pause timer incorrect: pause end after the end of the synchronized reaction");\par
\tab\tab\tab\tab\tab\}\par
\tab\tab\tab\tab\tab\par
\tab\tab\tab\tab\tab // check that pause end > pause start\par
\tab\tab\tab\tab\tab if(pauseStartTime >= pauseResumeTime)\par
\tab\tab\tab\tab\tab\{\par
\tab\tab\tab\tab\tab\tab arrayExtraPlayMM[i].setCustomValidity("Pause timer incorrect: resume time is smaller than pause time");\par
\tab\tab\tab\tab\tab\}\par
\tab\tab\tab\tab\}\par
\tab\tab\tab\}\par
\tab\tab\tab\par
\tab\tab\tab\par
\tab\tab\tab function validateSyncTimings()\par
\tab\tab\tab\{\par
\tab\tab\tab\tab var form = document.getElementById("form");\par
\par
\tab\tab\tab\tab form.syncStartTimeMM.setCustomValidity("");\par
\tab\tab\tab\tab form.syncStopTimeMM.setCustomValidity("");\par
\tab\tab\tab\tab\par
\tab\tab\tab\tab var syncStartTime = parseInt(form.syncStartTimeHH.value,10) * 3600 + parseInt(form.syncStartTimeMM.value,10) * 60 + parseInt(form.syncStartTimeSS.value,10);\par
\tab\tab\tab\tab var syncStopTime = parseInt(form.syncStopTimeHH.value,10) * 3600 + parseInt(form.syncStopTimeMM.value,10) * 60 + parseInt(form.syncStopTimeSS.value,10);\par
\tab\tab\tab\tab\par
\tab\tab\tab\tab if((syncStopTime > 0) &&  (syncStopTime <= syncStartTime))\par
\tab\tab\tab\tab\{\par
\tab\tab\tab\tab\tab form.syncStopTimeMM.setCustomValidity("Sync timers incorrect: sync stop time should be greater than sync start time");\par
\tab\tab\tab\tab\}\par
\tab\tab\tab\tab\par
\tab\tab\tab\tab //revalidate pause timing since they can be (in)validated with the sync timing changes\par
\tab\tab\tab\tab validatePauseTimings();\par
\tab\tab\tab\}\par
\tab\tab\tab\par
\tab\tab\tab\tab\tab\tab\par
\tab\tab\tab\tab\tab\tab\par
\tab\tab </script>\par
\tab </body>\par
</html>\par
