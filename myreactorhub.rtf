\tab\tab\tab function validateTalentUrl(inputElem)\par
\tab\tab\tab\{\tab\par
\tab\tab\tab\tab\par
\tab\tab\tab\tab var url = inputElem.value;\par
\tab\tab\tab\tab var provider = URLUtils.detectProvider(url);\par
\tab\tab\tab\tab\par
\tab\tab\tab\tab if(provider === "UNKNOWN")\par
\tab\tab\tab\tab\{\par
\tab\tab\tab\tab\tab // url is not valid\par
\tab\tab\tab\tab\tab console.log("bad talent video url!");\par
\tab\tab\tab\tab\tab inputElem.setCustomValidity("Talent URL not supported");\par
\tab\tab\tab\tab\}else\par
\tab\tab\tab\tab\{\par
\tab\tab\tab\tab\tab inputElem.setCustomValidity("");\par
\tab\tab\tab\tab\tab // check if we can extract the talent video start time from url\par
\tab\tab\tab\tab\tab var talentStartTimeInSeconds = 0;\par
\tab\tab\tab\tab\tab switch(provider)\{\par
\tab\tab\tab\tab\tab\tab case "YT":\par
\tab\tab\tab\tab\tab\tab\tab talentStartTimeInSeconds = URLUtils.YT.getStartTimeFromURL(url);\par
\tab\tab\tab\tab\tab\tab break;\par
\tab\tab\tab\tab\tab\tab case "DM":\par
\tab\tab\tab\tab\tab\tab\tab talentStartTimeInSeconds = URLUtils.DM.getStartTimeFromURL(url);\par
\tab\tab\tab\tab\tab\tab break;\par
\tab\tab\tab\tab\tab\tab case "VM":\par
\tab\tab\tab\tab\tab\tab\tab talentStartTimeInSeconds = URLUtils.VM.getStartTimeFromURL(url);\par
\tab\tab\tab\tab\tab\tab break;\par
\tab\tab\tab\tab\tab\tab case "Streamable":\par
\tab\tab\tab\tab\tab\tab\tab talentStartTimeInSeconds = URLUtils.Streamable.getStartTimeFromURL(url);\par
\tab\tab\tab\tab\tab\tab break;\par
\tab\tab\tab\tab\tab\}\par
\tab\tab\tab\tab\tab\par
\tab\tab\tab\tab\tab // update talent start time\par
\tab\tab\tab\tab\tab var talentStartHours = Math.floor(talentStartTimeInSeconds / 3600);\par
\tab\tab\tab\tab\tab talentStartTimeInSeconds %= 3600;\par
\tab\tab\tab\tab\tab var talentStartMinutes = Math.floor(talentStartTimeInSeconds / 60);\par
\tab\tab\tab\tab\tab talentStartTimeInSeconds %= 60;\par
\tab\tab\tab\tab\tab var talentStartSeconds = talentStartTimeInSeconds;\par
\tab\tab\tab\tab\tab\par
\tab\tab\tab\tab\tab document.getElementById("talentStartTimeHH").value = pad(talentStartHours, 2);\par
\tab\tab\tab\tab\tab document.getElementById("talentStartTimeMM").value = pad(talentStartMinutes, 2);\par
\tab\tab\tab\tab\tab document.getElementById("talentStartTimeSS").value = pad(talentStartSeconds, 2);\par
\tab\tab\tab\tab\tab\par
\tab\tab\tab\tab\}\par
\tab\tab\tab\}\par
\tab\tab\tab\par
